<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ynu.mapper.UserMapper">
	<select id="findUser" resultType="com.ynu.dto.User">
		select * from user where
		user_name = #{user_name}
	</select>
	<insert id="insertUser" parameterType="com.ynu.dto.User">
		INSERT INTO user(user_name,user_pw,user_phone) values(#{user_name},#{user_pw},#{user_phone})
	</insert>
</mapper>